{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "application": {
    "throttle": {
      "enabled": true,
      "defaultRequestsPerPeriod": 100,
      "periodInSeconds": 60
    }
  },
  "db": {
    "driver": "sqlite",
    "connectionString": "Data Source=fourCDesigner.db"
  },
  "system": {
    "defaultLanguage": "en",
    "passwordRules": {
      "minimumLength": 8,
      "requireCapitalLetter": true,
      "requireSmallLetter": true,
      "requireDigit": true,
      "requireSpecialSymbol": false
    }
  }
}